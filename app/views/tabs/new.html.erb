<h1>New tab</h1>

<% form_for(@tab) do |f| %>
  <%= f.error_messages %>
<div class="column">
  <%= f.label :title %><br />
  <%= f.text_field :title %>
</div>
<div class="column">
  <%= f.label :artist %><br />
  <%= f.text_field :artist %>
</div>
<div class="column">
  <%= f.label :tag %><br />
  <%= f.text_field :tag %>
</div>
<br />
<%= image_tag "fretboard.png", :usemap => "#fretboard", :id => "fretboard" %>
<div id="tab-actions">
<button type="button" onClick="clear_all()">Clear</button>
<button type="button" onClick="back_clear()">Backspace</button>
<button type="button" onClick='show_tab("tab_tab", "preview");'>Preview</button>
<%= f.submit "Save" %>
</div>
<%= f.text_area :tab %>

<% end %>

<div id="preview"></div>

